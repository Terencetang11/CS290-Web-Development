<h1>POST Request Received</h1>
<table style="border-style: solid;">
    <caption>URL Parameter Table</caption>
    <thead>
        <tr>
            <th style="border-style: solid;">Key</th>
            <th style="border-style: solid;">Value</th>
        </tr>
    </thead>
    <tbody>
        {{#each urlList}}
            <tr>
                <td style="border-style: solid;">{{this.name}}</td>
                <td style="border-style: solid;">{{this.value}}</td>
            </tr>>
        {{/each}}
    </tbody>
</table>

<table style="border-style: solid;">
    <caption>POST Body Parameter Table</caption>
    <thead>
        <tr>
            <th style="border-style: solid;">Key</th>
            <th style="border-style: solid;">Value</th>
        </tr>
    </thead>
    <tbody>
        {{#each bodyList}}
            <tr>
                <td style="border-style: solid;">{{this.name}}</td>
                <td style="border-style: solid;">{{this.value}}</td>
            </tr>>
        {{/each}}
    </tbody>
</table>